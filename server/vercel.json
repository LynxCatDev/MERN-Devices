{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/api/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=900, s-maxage=900, stale-while-revalidate=59, immutable"
        }
      ]
    }
  ]
}
